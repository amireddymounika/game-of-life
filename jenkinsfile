pipeline {
  agent {label 'GOL'}
  stages {
    
    stage ('SCM') {
       steps {
              git branch: 'master', url: 'https://github.com/amireddymounika/game-of-life.git'
       }
    }
      stage ('build') {
         steps {
            sh 'mvn package'
          }
      }
  }
  post {
     success {
       archive '**/*.war'
       junit '**/TEST-*.xml'
     }
 }

}